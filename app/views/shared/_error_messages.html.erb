<div class="panel panel-danger" id="error_explanation">
      <div class="panel-heading">
        <h3 class="panel-title"><%=pluralize(@user.errors.count, "error")%></h3>
      </div>
      <div class="panel-body">
      <ul>
        <%@user.errors.full_messages.each do |error|%>
		<li><%=error%></li>
		<%end%>
		</ul>
      </div>
    </div>
