<%provide(:title,"Log in")%>

<div class="row">
<div class="col-md-4 col-md-offset-4 bg-success", id="bbb">
<h3 class="col-md-offset-4">Log in</h3>
<%=form_for(:session,url: login_path) do |f|%>

<%=f.label :email%>
<%=f.email_field :email, class:"form-control"%>
<%=f.label :password%>
<%=f.password_field :password,class:"form-control"%>
<%=f.submit "Log in",class:"btn btn-primary", id:"rrr"%>
<%end%>
</div>
</div>